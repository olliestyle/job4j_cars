<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>ru.job4j.controller.LogInServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>ru.job4j.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>ru.job4j.controller.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BrandServlet</servlet-name>
    <servlet-class>ru.job4j.controller.BrandServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BodyTypeServlet</servlet-name>
    <servlet-class>ru.job4j.controller.BodyTypeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TransmissionServlet</servlet-name>
    <servlet-class>ru.job4j.controller.TransmissionServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ModelServlet</servlet-name>
    <servlet-class>ru.job4j.controller.ModelServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AllAdsServlet</servlet-name>
    <servlet-class>ru.job4j.controller.AllAdsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PhotoDownloadServlet</servlet-name>
    <servlet-class>ru.job4j.controller.PhotoDownloadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegistrationServlet</servlet-name>
    <url-pattern>/reg.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BrandServlet</servlet-name>
    <url-pattern>/brands.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BodyTypeServlet</servlet-name>
    <url-pattern>/bodyTypes.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TransmissionServlet</servlet-name>
    <url-pattern>/transmissions.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ModelServlet</servlet-name>
    <url-pattern>/models.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AllAdsServlet</servlet-name>
    <url-pattern>/ads.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PhotoDownloadServlet</servlet-name>
    <url-pattern>/download.do</url-pattern>
  </servlet-mapping>
</web-app>